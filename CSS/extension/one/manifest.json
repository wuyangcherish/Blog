{
	"manifest_version": 2,
	"name": "TESTONE",
	"version": "1.0",
	"description": "让你永远点不到Google的搜索按钮",
	"icons": {
		"32": "chicken_32.png",
		"64": "chicken_64.png"
	},
	"content_scripts": [{
		"matches": ["*://www.google.com.vn/*"],
		"js": ["js/cannot_touch.js"]
	}],
	"browser_action": {
		"default_icon": {
			"19": "chicken_64.png"
		},
		"default_title": "test"
	}
}